{
  "name": "business-management-app",
  "version": "1.0.0",
  "description": "An all inclusive business operations application",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "updateQrIds": "cd server && node seeders/updateQrIds.js",
    "build": "cd client && npm run build"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-sherrill/business-management.git"
  },
  "author": "Morgan Sherrill",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/m-sherrill/business-management/issues"
  },
  "homepage": "https://github.com/m-sherrill/business-management#readme"
}
